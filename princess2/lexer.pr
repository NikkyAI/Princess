export type TokenType = enum {
    O_PAREN
    C_PAREN
    INTEGER
    FLOAT
    STRING
}

export type Token = struct {
    tpe: TokenType
    value: *
}

export type TokenList = struct {
    value: Token
    next: *TokenList
}

def string_token(s: string) -> Token {
    return {
        tpe = TokenType::STRING, 
        value = s
    } !Token
}

export def lex(s: string) -> TokenList {
    let tl = {
        value = string_token(s),
        next = null
    } !TokenList

    return tl
}